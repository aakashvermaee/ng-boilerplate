{
  "name": "ng-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run env-builder && npm run prettier",
    "start": "ng run ng-boilerplate:serve:development",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/ng-boilerplate/server/main.js",
    "prod:ssr": "npm run env-builder && ng build --prod && ng run ng-boilerplate:server:production",
    "stage:ssr": "npm run env-builder && ng build --configuration=staging && ng run ng-boilerplate:server:staging",
    "dev:ssr": "npm run env-builder && ng build --configuration=development && ng run ng-boilerplate:server:development",
    "prerender": "ng run ng-boilerplate:prerender",
    "prettier": "prettier --write src",
    "env-builder": "node env-builder.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/platform-server": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@nguniversal/express-engine": "^11.0.1",
    "bootstrap": "^4.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "helmet": "^4.2.0",
    "lodash": "^4.17.20",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
